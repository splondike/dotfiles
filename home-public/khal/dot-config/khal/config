[calendars]

[[my_calendars_local]]
path = ~/Documents/calendars/*
type = discover

[locale]
timeformat = %H:%M
dateformat = %Y-%m-%d
longdateformat = %Y-%m-%d
datetimeformat = %Y-%m-%d %H:%M
longdatetimeformat = %Y-%m-%d %H:%M

[default]
highlight_event_days = True

[highlight_days]
color = light blue

[keybindings]
down = e
up = u
left = n
right = i
new = c
export = j
